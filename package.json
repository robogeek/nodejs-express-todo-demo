{
  "name": "nodejs-express-todo-demo",
  "version": "1.0.0",
  "description": "Sample TODO application written with Node.js and Express",
  "config": {
    "port": "4000",
    "dbconfig": "models/sequelize-sqlite.yaml",
    "tagname": "first-dockerize",
    "tagnameredis": "first-dockerize-redis",
    "context": "default"
  },
  "scripts": {
    "env": "env",
    "start": "cross-env PORT=${npm_package_config_port} SEQUELIZE_CONNECT=${npm_package_config_dbconfig} node app.mjs",
    "docker-build": "docker --context ${npm_package_config_context} build -t todo-app .",
    "docker-run": "docker --context ${npm_package_config_context} run --name todo-app -p 4000:80 todo-app",
    "docker-tag": "npm-run-all tag:*",
    "tag:latest": "docker --context ${npm_package_config_context} tag todo-app robogeek/todo-app:latest",
    "tag:dockerize": "docker --context ${npm_package_config_context} tag todo-app robogeek/todo-app:${npm_package_config_tagname}",
    "tag:redis": "docker --context ${npm_package_config_context} tag todo-app robogeek/todo-app:${npm_package_config_tagnameredis}",
    "docker-push": "npm-run-all push:*",
    "push:latest": "docker --context ${npm_package_config_context} push robogeek/todo-app:latest",
    "push:dockerize": "docker --context ${npm_package_config_context} push robogeek/todo-app:${npm_package_config_tagname}",
    "push:redis": "docker --context ${npm_package_config_context} push robogeek/todo-app:${npm_package_config_tagnameredis}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robogeek/nodejs-express-todo-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robogeek/nodejs-express-todo-demo/issues"
  },
  "homepage": "https://github.com/robogeek/nodejs-express-todo-demo#readme",
  "dependencies": {
    "bootstrap": "^5.0.0-alpha1",
    "bootstrap-icons": "^1.0.0-alpha5",
    "connect-loki": "^1.1.0",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "cross-var": "^1.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-session-sequelize": "^2.3.0",
    "express-socket.io-session": "^1.3.5",
    "helmet": "^4.1.0",
    "js-yaml": "^3.14.0",
    "memorystore": "^1.6.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "nunjucks": "^3.2.2",
    "popper.js": "^1.16.1",
    "redis": "^3.0.2",
    "sequelize": "^6.3.4",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.4.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
